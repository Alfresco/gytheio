<?xml version='1.0' encoding='UTF-8'?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
          http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="tempFileContentTransport" class="org.alfresco.messaging.content.transport.ContentTransportImpl">
        <property name="mimetypeService" ref="mimetypeService" />
        <property name="contentReferenceHandler">
            <bean class="org.alfresco.content.handler.FileContentReferenceHandlerImpl" />
        </property>
    </bean>
    
</beans>